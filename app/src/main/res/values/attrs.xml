<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SquareLoading">
        <attr name="squareColor" format="color"/>
        <attr name="squareSize" format="dimension"/>
        <attr name="squareCorner" format="dimension"/>
        <attr name="dividerSize" format="dimension"/>
        <attr name="xCount" format="integer"/>
        <attr name="yCount" format="integer"/>
    </declare-styleable>

    <string-array name="bubble">
        <item>#41ffffff</item>
        <item>#18ffffff</item>
        <item>#1fffffff</item>
        <item>#25ffffff</item>
    </string-array>

    <string-array name="BubbleAnimations">
        <item>com.github.glomadrian.grav.generator.animation.SideToSideAnimator</item>
        <item>com.github.glomadrian.grav.generator.animation.ShakeAnimator</item>
    </string-array>
</resources>